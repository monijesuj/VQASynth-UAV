<?xml version="1.0" ?>
<sdf version="1.6">
  <world name="spatial_navigation_world">
    
    <!-- Physics settings optimized for drone simulation -->
    <physics name='default_physics' default='0' type='ode'>
      <ode>
        <solver>
          <type>quick</type>
          <iters>150</iters>
          <sor>1.3</sor>
        </solver>
        <constraints>
          <cfm>0.00001</cfm>
          <erp>0.2</erp>
          <contact_max_correcting_vel>1000</contact_max_correcting_vel>
          <contact_surface_layer>0.01</contact_surface_layer>
        </constraints>
      </ode>
      <max_step_size>0.004</max_step_size>
      <real_time_factor>1.0</real_time_factor>
      <real_time_update_rate>250</real_time_update_rate>
      <gravity>0 0 -9.81</gravity>
    </physics>

    <!-- Scene settings with realistic lighting -->
    <scene>
      <ambient>0.4 0.4 0.4 1</ambient>
      <background>0.7 0.7 1.0 1</background>
      <shadows>1</shadows>
    </scene>

    <!-- Sun for realistic lighting -->
    <include>
      <uri>model://sun</uri>
    </include>

    <!-- Ground plane -->
    <model name="ground_plane">
      <static>true</static>
      <link name="link">
        <collision name="collision">
          <geometry>
            <plane>
              <normal>0 0 1</normal>
              <size>200 200</size>
            </plane>
          </geometry>
        </collision>
        <visual name="visual">
          <geometry>
            <plane>
              <normal>0 0 1</normal>
              <size>200 200</size>
            </plane>
          </geometry>
          <material>
            <script>
              <uri>file://media/materials/scripts/gazebo.material</uri>
              <name>Gazebo/Grey</name>
            </script>
          </material>
        </visual>
      </link>
    </model>

    <!-- Warehouse Building for Indoor Navigation Testing -->
    <model name="warehouse">
      <pose>0 0 0 0 0 0</pose>
      <static>true</static>
      <link name="walls">
        <!-- Main structure walls -->
        <collision name="wall_front">
          <pose>0 -15 2.5 0 0 0</pose>
          <geometry>
            <box>
              <size>30 0.2 5</size>
            </box>
          </geometry>
        </collision>
        <visual name="wall_front_visual">
          <pose>0 -15 2.5 0 0 0</pose>
          <geometry>
            <box>
              <size>30 0.2 5</size>
            </box>
          </geometry>
          <material>
            <script>
              <uri>file://media/materials/scripts/gazebo.material</uri>
              <name>Gazebo/Bricks</name>
            </script>
          </material>
        </visual>

        <collision name="wall_back">
          <pose>0 15 2.5 0 0 0</pose>
          <geometry>
            <box>
              <size>30 0.2 5</size>
            </box>
          </geometry>
        </collision>
        <visual name="wall_back_visual">
          <pose>0 15 2.5 0 0 0</pose>
          <geometry>
            <box>
              <size>30 0.2 5</size>
            </box>
          </geometry>
          <material>
            <script>
              <uri>file://media/materials/scripts/gazebo.material</uri>
              <name>Gazebo/Bricks</name>
            </script>
          </material>
        </visual>

        <collision name="wall_left">
          <pose>-15 0 2.5 0 0 0</pose>
          <geometry>
            <box>
              <size>0.2 30 5</size>
            </box>
          </geometry>
        </collision>
        <visual name="wall_left_visual">
          <pose>-15 0 2.5 0 0 0</pose>
          <geometry>
            <box>
              <size>0.2 30 5</size>
            </box>
          </geometry>
          <material>
            <script>
              <uri>file://media/materials/scripts/gazebo.material</uri>
              <name>Gazebo/Bricks</name>
            </script>
          </material>
        </visual>

        <collision name="wall_right">
          <pose>15 0 2.5 0 0 0</pose>
          <geometry>
            <box>
              <size>0.2 30 5</size>
            </box>
          </geometry>
        </collision>
        <visual name="wall_right_visual">
          <pose>15 0 2.5 0 0 0</pose>
          <geometry>
            <box>
              <size>0.2 30 5</size>
            </box>
          </geometry>
          <material>
            <script>
              <uri>file://media/materials/scripts/gazebo.material</uri>
              <name>Gazebo/Bricks</name>
            </script>
          </material>
        </visual>

        <!-- Roof -->
        <collision name="roof">
          <pose>0 0 5.1 0 0 0</pose>
          <geometry>
            <box>
              <size>30 30 0.2</size>
            </box>
          </geometry>
        </collision>
        <visual name="roof_visual">
          <pose>0 0 5.1 0 0 0</pose>
          <geometry>
            <box>
              <size>30 30 0.2</size>
            </box>
          </geometry>
          <material>
            <script>
              <uri>file://media/materials/scripts/gazebo.material</uri>
              <name>Gazebo/Grey</name>
            </script>
          </material>
        </visual>
      </link>
    </model>

    <!-- Navigation Obstacles - Boxes and Pillars -->
    <!-- Box Obstacle 1 -->
    <model name="box_obstacle_1">
      <pose>-8 -5 0.5 0 0 0</pose>
      <static>true</static>
      <link name="link">
        <collision name="collision">
          <geometry>
            <box>
              <size>2 2 1</size>
            </box>
          </geometry>
        </collision>
        <visual name="visual">
          <geometry>
            <box>
              <size>2 2 1</size>
            </box>
          </geometry>
          <material>
            <script>
              <uri>file://media/materials/scripts/gazebo.material</uri>
              <name>Gazebo/Wood</name>
            </script>
          </material>
        </visual>
      </link>
    </model>

    <!-- Box Obstacle 2 -->
    <model name="box_obstacle_2">
      <pose>8 5 0.75 0 0 0</pose>
      <static>true</static>
      <link name="link">
        <collision name="collision">
          <geometry>
            <box>
              <size>1.5 1.5 1.5</size>
            </box>
          </geometry>
        </collision>
        <visual name="visual">
          <geometry>
            <box>
              <size>1.5 1.5 1.5</size>
            </box>
          </geometry>
          <material>
            <script>
              <uri>file://media/materials/scripts/gazebo.material</uri>
              <name>Gazebo/Red</name>
            </script>
          </material>
        </visual>
      </link>
    </model>

    <!-- Tall Pillar Obstacle -->
    <model name="pillar_obstacle">
      <pose>0 8 1.5 0 0 0</pose>
      <static>true</static>
      <link name="link">
        <collision name="collision">
          <geometry>
            <cylinder>
              <radius>0.5</radius>
              <length>3</length>
            </cylinder>
          </geometry>
        </collision>
        <visual name="visual">
          <geometry>
            <cylinder>
              <radius>0.5</radius>
              <length>3</length>
            </cylinder>
          </geometry>
          <material>
            <script>
              <uri>file://media/materials/scripts/gazebo.material</uri>
              <name>Gazebo/Blue</name>
            </script>
          </material>
        </visual>
      </link>
    </model>

    <!-- Corridor Obstacles for Navigation Testing -->
    <model name="corridor_wall_left">
      <pose>-3 0 1 0 0 1.57</pose>
      <static>true</static>
      <link name="link">
        <collision name="collision">
          <geometry>
            <box>
              <size>10 0.2 2</size>
            </box>
          </geometry>
        </collision>
        <visual name="visual">
          <geometry>
            <box>
              <size>10 0.2 2</size>
            </box>
          </geometry>
          <material>
            <script>
              <uri>file://media/materials/scripts/gazebo.material</uri>
              <name>Gazebo/Grey</name>
            </script>
          </material>
        </visual>
      </link>
    </model>

    <model name="corridor_wall_right">
      <pose>3 0 1 0 0 1.57</pose>
      <static>true</static>
      <link name="link">
        <collision name="collision">
          <geometry>
            <box>
              <size>10 0.2 2</size>
            </box>
          </geometry>
        </collision>
        <visual name="visual">
          <geometry>
            <box>
              <size>10 0.2 2</size>
            </box>
          </geometry>
          <material>
            <script>
              <uri>file://media/materials/scripts/gazebo.material</uri>
              <name>Gazebo/Grey</name>
            </script>
          </material>
        </visual>
      </link>
    </model>

    <!-- Trees for Outdoor Navigation -->
    <model name="tree_1">
      <pose>20 20 0 0 0 0</pose>
      <static>true</static>
      <link name="trunk">
        <collision name="trunk_collision">
          <pose>0 0 2 0 0 0</pose>
          <geometry>
            <cylinder>
              <radius>0.3</radius>
              <length>4</length>
            </cylinder>
          </geometry>
        </collision>
        <visual name="trunk_visual">
          <pose>0 0 2 0 0 0</pose>
          <geometry>
            <cylinder>
              <radius>0.3</radius>
              <length>4</length>
            </cylinder>
          </geometry>
          <material>
            <script>
              <uri>file://media/materials/scripts/gazebo.material</uri>
              <name>Gazebo/Wood</name>
            </script>
          </material>
        </visual>
        
        <collision name="canopy_collision">
          <pose>0 0 5 0 0 0</pose>
          <geometry>
            <sphere>
              <radius>2</radius>
            </sphere>
          </geometry>
        </collision>
        <visual name="canopy_visual">
          <pose>0 0 5 0 0 0</pose>
          <geometry>
            <sphere>
              <radius>2</radius>
            </sphere>
          </geometry>
          <material>
            <script>
              <uri>file://media/materials/scripts/gazebo.material</uri>
              <name>Gazebo/Green</name>
            </script>
          </material>
        </visual>
      </link>
    </model>

    <model name="tree_2">
      <pose>25 -20 0 0 0 0</pose>
      <static>true</static>
      <link name="trunk">
        <collision name="trunk_collision">
          <pose>0 0 2.5 0 0 0</pose>
          <geometry>
            <cylinder>
              <radius>0.25</radius>
              <length>5</length>
            </cylinder>
          </geometry>
        </collision>
        <visual name="trunk_visual">
          <pose>0 0 2.5 0 0 0</pose>
          <geometry>
            <cylinder>
              <radius>0.25</radius>
              <length>5</length>
            </cylinder>
          </geometry>
          <material>
            <script>
              <uri>file://media/materials/scripts/gazebo.material</uri>
              <name>Gazebo/Wood</name>
            </script>
          </material>
        </visual>
        
        <collision name="canopy_collision">
          <pose>0 0 6 0 0 0</pose>
          <geometry>
            <sphere>
              <radius>1.8</radius>
            </sphere>
          </geometry>
        </collision>
        <visual name="canopy_visual">
          <pose>0 0 6 0 0 0</pose>
          <geometry>
            <sphere>
              <radius>1.8</radius>
            </sphere>
          </geometry>
          <material>
            <script>
              <uri>file://media/materials/scripts/gazebo.material</uri>
              <name>Gazebo/Green</name>
            </script>
          </material>
        </visual>
      </link>
    </model>

    <!-- Power Line Simulation (Horizontal obstacle) -->
    <model name="power_line">
      <pose>0 -25 4 0 0 0</pose>
      <static>true</static>
      <link name="cable">
        <collision name="collision">
          <geometry>
            <cylinder>
              <radius>0.02</radius>
              <length>40</length>
            </cylinder>
          </geometry>
        </collision>
        <visual name="visual">
          <geometry>
            <cylinder>
              <radius>0.02</radius>
              <length>40</length>
            </cylinder>
          </geometry>
          <material>
            <script>
              <uri>file://media/materials/scripts/gazebo.material</uri>
              <name>Gazebo/Black</name>
            </script>
          </material>
        </visual>
      </link>
    </model>

    <!-- Spatial Reasoning Drone - positioned safely outside warehouse -->
    <include>
      <uri>model://spatial_reasoning_drone</uri>
      <name>spatial_reasoning_drone</name>
      <pose>-25 -25 1.0 0 0 0</pose>  <!-- Safe starting position: 25m away from warehouse center -->
    </include>

    <!-- Wind plugin for realistic flight conditions -->
    <plugin name="wind" filename="libgazebo_wind_plugin.so">
      <frameId>base_link</frameId>
      <robotNamespace></robotNamespace>
      <xyzOffset>0 0 0</xyzOffset>
      <windDirection>0 1 0</windDirection>
      <windVelocityMax>5</windVelocityMax>
      <windVelocityMin>0</windVelocityMin>
      <windDirectionVariance>15</windDirectionVariance>
    </plugin>

    <!-- GUI settings -->
    <gui fullscreen='0'>
      <camera name='user_camera'>
        <pose frame=''>-20 -20 15 0 0.5 0.7</pose>
        <view_controller>orbit</view_controller>
        <projection_type>perspective</projection_type>
      </camera>
    </gui>

  </world>
</sdf>